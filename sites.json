{
	"amazon": {
		"match": "amazon\\.cn|z\\.cn",
		"sitename": "亚马逊中国",
		"append": ["td#actualPriceContent","span.priceLarge"],
		"price": "$('span#dealPriceValue').text()||$('b.priceLarge:first').text()||$('span.priceLarge:first').text()",
		"title": "<span id=\"btAsinTitle\">(.*?)<\\/span>",
		"image": "<img src=\"(.*?)\"\\s+id=\"original-main-image\"",
		"fetchPrice": "\"dealPrice\"[\\s\\S]*\"price\" : \"|<b class=\"priceLarge\">|<span class=\"priceLarge\">"
	},
	"amazonUSA": {
		"match": "amazon\\.com",
		"sitename": "亚马逊美国",
		"append": ["span.a-color-price.a-size-large","td#actualPriceContent"],
		"price": "$('b.priceLarge:first').text()||$('span.a-color-price.a-size-large').text()",
		"title": "$('span#btAsinTitle').text()||$('h1').text()",
		"image": "$('img#main-image').attr('src')",
		"fetchPrice": "<b class=\"priceLarge\">|<span class=\"a-color-price a-size-large\">"
	},
	"taobao": {
		"match": "taobao\\.com\\/item",
		"sitename": "淘宝",
		"append": ["#J_StrPriceModBox", "#J_PromoPrice"],
		"price": "$('strong.tb-rmb-num:first').text()||$('em.tb-rmb-num:first').text()",
		"title": "$('.tb-detail-hd h3').text()",
		"image": "$('img#J_ImgBooth').attr('src')",
		"fetchPrice": "class=\"tb-rmb-num\">"
	},
	"tmall": {
		"match": "tmall\\.com\\/item",
		"sitename": "天猫",
		"append": ["#J_StrPriceModBox", "#J_PromoPrice"],
		"price": "$('div#J_PromoBox').text()||$('strong#J_StrPrice').text()",
		"title": "$('.tb-detail-hd h3').text()",
		"image": "$('#J_ImgBooth').attr('src')",
		"fetchPrice": "<em class=\"tb-rmb-num\">|class=\"J_originalPrice\">|'reservePrice' : '|'defaultItemPrice':'"
	},
	"jd": {
		"match": "item\\.jd\\.com|360buy\\.com",
		"sitename": "京东",
		"append": ["#jd-price"],
		"price": "$('strong#jd-price').text()",
		"title": "$('div#name h1').text()",
		"image": "$('div.jqzoom img').attr('src')",
		"fetchPrice": "18px\">&yen;",
		"productID": "\\/(\\d+)\\.html",
		"MobileURL": "http://m.jd.com/product/888888.html"
	},
	"dangdang": {
		"match": "product\\.dangdang\\.com",
		"sitename": "当当",
		"append": ["b#d_price"],
		"price": "$('i#promo_price').text()||$('span#salePriceTag').text()||$('b#d_price').text()",
		"title": "$('div.head h1').text()",
		"image": "$('img#largePic').attr('src')",
		"fetchPrice": "id=\"promo_price\">&yen;|<span id=\"salePriceTag\">&yen;|<span class=\"yen\">&yen;<\\/span>"
	},
	"51buy": {
		"match": "item\\.51buy\\.com",
		"sitename": "易迅",
		"append": ["li.li.item_icson"],
		"price": "$('strong.price_font:first').text()",
		"title": "$('div.property.id_promotion h1').text()",
		"image": "$('img#smallImage').attr('src')",
		"fetchPrice": "<strong class=\"price_font\">"
	},
	"suning": {
		"match": "suning\\.com\\/emall",
		"sitename": "苏宁易购",
		"append": ["span#mainPrice", "span#bookprdprice"],
		"price": "$('span#mainPrice').text()",
		"title": "$('h1').text()",
		"image": "$('div.zoomple img').attr('src')",
		"fetchPrice": "&currPrice="
	},
	"newegg": {
		"match": "newegg\\.com\\.cn\\/Product",
		"sitename": "新蛋",
		"append": ["dd.neweggPrice :first"],
		"price": "$('body').html().match(\/,pvalues:([\\d,]+\\.\\d{2})\/)[1]",
		"title": "$('h1').text()",
		"image": "$('img#midImg').attr('src')",
		"fetchPrice": ",pvalues:"
	},
	"yihaodian": {
		"match": "yihaodian\\.com\\/item|1mall\\.com\\/item",
		"sitename": "一号店",
		"append": ["span#productFacadePrice", "span#nonMemberPrice"],
		"price": "$('input#salePrice').val()",
		"title": "$('h1').text()",
		"image": "$('img#productImg').attr('src')",
		"fetchPrice": "id=\"salePrice\"  value=\""
	}
}
